{
 "manifest_version": 3,
 "version": "1.0",
 "name": "Time tracker",
 "action": {
  "default_popup": "index.html",
  "default_icon": "images/openscriptIcon.png"
 },
 "permissions": ["scripting", "tabs", "download", "exports", "system.storage", "storage", "syncFileSystem", "webRequest", "fileSystem"],
 "background": {
 "presistent": true
 } 
}